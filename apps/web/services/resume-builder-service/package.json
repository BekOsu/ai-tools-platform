{
  "name": "resume-builder-service",
  "version": "1.0.0",
  "description": "AI-powered resume builder service with PDF generation",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "docker:build": "docker build -t resume-builder-service .",
    "docker:run": "docker run -p 8006:8006 resume-builder-service"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.14.0",
    "puppeteer": "^21.5.2",
    "rate-limiter-flexible": "^3.0.8",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0"
  },
  "keywords": [
    "ai",
    "ats",
    "builder",
    "optimization",
    "pdf",
    "puppeteer",
    "resume",
    "typescript",
    "express"
  ],
  "author": "AI Platform Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}